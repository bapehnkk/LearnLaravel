{
    "name": "shahvirag/laravel-ui-bootstrap",
    "description": "Bootstrap Starter template for Laravel",
    "keywords": [
        "laravel",
        "Bootstrap-4",
        "laravel-ui",
        "laravel-fortify-bootstrap"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Virag Shah",
            "email": "shahvirag07@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "illuminate/support": "^8.0",
        "laravel/fortify": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Shahvirag\\LaravelUiBootstrap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shahvirag\\LaravelUiBootstrap\\Tests\\": "test/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shahvirag\\LaravelUiBootstrap\\LaravelUiBootstrapServiceProvider"
            ]
        }
    }
}
